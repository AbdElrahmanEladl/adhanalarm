<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="200dp" android:layout_width="160dp"
	android:orientation="vertical" android:background="@drawable/timetable_widget_background"
	android:id="@+id/timetable_widget">
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:orientation="vertical"
		android:paddingTop="11dp" android:paddingLeft="18dp">
		<LinearLayout android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:orientation="horizontal">
			<LinearLayout android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:orientation="vertical">
				<TextView android:text="@string/fajr" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="@android:color/black"
					android:paddingTop="3dp" android:paddingBottom="3dp" />
				<TextView android:text="@string/sunrise"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:textColor="@android:color/black" android:paddingTop="3dp"
					android:paddingBottom="3dp" />
				<TextView android:text="@string/dhuhr"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:textColor="@android:color/black" android:paddingTop="3dp"
					android:paddingBottom="3dp" />
				<TextView android:text="@string/asr" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="@android:color/black"
					android:paddingTop="3dp" android:paddingBottom="3dp" />
				<TextView android:text="@string/maghrib"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:textColor="@android:color/black" android:paddingTop="3dp"
					android:paddingBottom="3dp" />
				<TextView android:text="@string/ishaa"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:textColor="@android:color/black" android:paddingTop="3dp"
					android:paddingBottom="3dp" />
				<TextView android:text="@string/next_fajr"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:textColor="@android:color/black" android:paddingTop="3dp"
					android:paddingBottom="3dp" />
			</LinearLayout>
			<LinearLayout android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:orientation="vertical"
				android:paddingLeft="3dp">
				<TextView android:id="@+id/fajr" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="@android:color/black"
					android:paddingTop="3dp" android:paddingBottom="3dp" />
				<TextView android:id="@+id/sunrise" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="@android:color/black"
					android:paddingTop="3dp" android:paddingBottom="3dp" />
				<TextView android:id="@+id/dhuhr" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="@android:color/black"
					android:paddingTop="3dp" android:paddingBottom="3dp" />
				<TextView android:id="@+id/asr" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="@android:color/black"
					android:paddingTop="3dp" android:paddingBottom="3dp" />
				<TextView android:id="@+id/maghrib" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="@android:color/black"
					android:paddingTop="3dp" android:paddingBottom="3dp" />
				<TextView android:id="@+id/ishaa" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="@android:color/black"
					android:paddingTop="3dp" android:paddingBottom="3dp" />
				<TextView android:id="@+id/next_fajr" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="@android:color/black"
					android:paddingTop="3dp" android:paddingBottom="3dp" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>